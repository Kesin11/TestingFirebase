{
  "name": "testingfirebase",
  "version": "1.0.0",
  "description": "Testing firebase project",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:ci": "jest --reporters=default --reporters=jest-junit --ci --runInBand ",
    "firebase:use_test": "firebase use test",
    "auth:clear": "ts-node scripts/clear_auth_users.ts",
    "auth:import": "firebase auth:import auth_export.json",
    "auth:export": "firebase auth:export auth_export.json --format=json",
    "firestore:deploy": "firebase deploy --only firestore",
    "firestore:insert_dummy": "ts-node scripts/insert_dummy_data.ts",
    "firestore:delete_all": "firebase firestore:delete --all-collections",
    "emulators:setup": "firebase setup:emulators:firestore",
    "emulators:start": "firebase emulators:start --only firestore",
    "emulators:test": "firebase emulators:exec --only firestore 'npm run test'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kesin11/TestingFirebase.git"
  },
  "author": "kesin1202000@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Kesin11/TestingFirebase/issues"
  },
  "homepage": "https://github.com/Kesin11/TestingFirebase#readme",
  "private": true,
  "dependencies": {
    "@firebase/testing": "0.15.0",
    "@types/node": "12.12.7",
    "firebase": "7.3.0",
    "firebase-admin": "8.7.0",
    "uuid": "3.3.3"
  },
  "devDependencies": {
    "@types/jest": "24.0.22",
    "@types/uuid": "3.4.6",
    "firebase-tools": "7.7.0",
    "jest": "24.9.0",
    "jest-junit": "9.0.0",
    "ts-jest": "24.1.0",
    "ts-node": "8.4.1",
    "typescript": "3.7.2"
  }
}
